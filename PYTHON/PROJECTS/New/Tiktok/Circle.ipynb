    "from turtle import *\n",
    "from colorsys import *\n",
    "x = 0\n",
    "bgcolor('black')\n",
    "speed(0)\n",
    "hideturtle()\n",
    "for i in range(100):\n",
    "    color(hsv_to_rgb(0.6, x, 1))\n",
    "    circle(i*2)\n",
    "    lt(5)\n",
    "    x += 0.01\n",
    "done()"
